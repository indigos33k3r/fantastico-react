{
  "name": "fantastico",
  "version": "1.0.0",
  "description": "Hassle-free raw Webpack 4 boilerplate for a quick dev setup experience",
  "author": "Cezar Augusto",
  "license": "MIT",
  "homepage": "https://github.com/cezaraugusto/fantastico",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.prod.js",
    "check-security": "nsp",
    "lint": "standard --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://cezaraugusto@github.com/cezaraugusto/fantastico.git"
  },
  "dependencies": {
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nsp": "^3.2.1",
    "standard": "^11.0.1",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  }
}
